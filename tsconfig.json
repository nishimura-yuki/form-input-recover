{
  "compilerOptions": {
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "experimentalDecorators": true,
    "moduleResolution": "Node",
    "declaration": false,
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "es2015",
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "es2015"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}