{
  "compilerOptions": {
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "experimentalDecorators": true,
    "moduleResolution": "Node",
    "declaration": false,
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "es2015",
    "target": "es6"
  },
  "exclude": [
    "node_modules"
  ]
}